{
  "applicationId": "showrunner",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/showrunner/build",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*", "os.*"],
  "modes": {
    "window": {
      "title": "Showrunner",
      "width": 1920,
      "height": 1080,
      "maximize": true,
      "enableInspector": true,
      "icon": "/showrunner/public/logo192.png"
    }
  },
  "cli": {
    "binaryName": "Showrunner-client",
    "resourcesPath": "/showrunner/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/showrunner/public/neutralino.js",
    "binaryVersion": "4.4.0",
    "clientVersion": "3.3.0",
    "frontendLibrary": {
      "patchFile": "/showrunner/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}
